class Threat{
  int type;
  float approachTime;
  float approachAngle;
  float approachDistance;
  float approachSpeed;
  public Threat(int t, float at, float aa, float ad, float as){
    type = t;
    approachTime = at;
    approachAngle = aa;
    approachDistance = ad;
    approachSpeed = as;
  }
}